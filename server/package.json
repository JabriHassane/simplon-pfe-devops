{
	"name": "server",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"dev": "NODE_ENV=development tsx watch src/index.ts",
		"build": "tsc",
		"start": "NODE_ENV=production node dist/server/src/index.js",
		"db:generate": "dotenv -e .env.development -- prisma generate",
		"db:migrate": "dotenv -e .env.development -- prisma migrate dev",
		"db:push": "dotenv -e .env.development -- prisma db push",
		"db:studio": "dotenv -e .env.development -- prisma studio",
		"db:seed": "dotenv -e .env.development -- tsx prisma/seed.ts",
		"db:generate:prod": "dotenv -e .env.production -- prisma generate",
		"db:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
		"db:studio:prod": "dotenv -e .env.production -- prisma studio",
		"db:seed:prod": "dotenv -e .env.production -- tsx prisma/seed.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"@prisma/client": "^5.7.1",
		"@types/ms": "^2.1.0",
		"bcryptjs": "^3.0.2",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"express": "^4.18.2",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"ms": "^2.1.3",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/morgan": "^1.9.10",
		"@types/node": "^20.10.5",
		"dotenv-cli": "^10.0.0",
		"prisma": "^5.7.1",
		"tsx": "^4.20.3",
		"typescript": "^5.3.3"
	}
}
